{
    "all": true,
    "branches": 0,
    "lines": 0,
    "functions": 0,
    "statements": 0,
    "check-coverage": true,
    "include": ["dist"],
    "exclude": [
        ".ignore",
        "coverage",
        "**/*.spec.js",
        "**/*.d.ts",
        "dist/templates/**.js",
        "dist/cli/**.js",
        "dist/index.js"
    ],
    "report-dir": "./coverage/",
    "cache": false,
    "source-map": true,
    "produce-source-map": true,
    "exclude-after-remap": false
}
